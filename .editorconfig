# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

[*.dart]
indent_style = tab
indent_size = 4
charset = utf-8
trim_trailing_whitespace = true

[*.yaml]
indent_style = space
indent_size = 2
